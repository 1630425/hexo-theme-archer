<section class="posts">
  <% page.posts.forEach(function (post) { %>
        <% var isExcerpt; %>
        <% if (post.excerpt){ %>
        <% isExcerpt = true %>
        <% } else { %>
        <% isExcerpt = false %>
        <% } %>
        <%- partial('_partial/base-post', { _page :post, isExcerpt : isExcerpt}) %>
    <% }) %>
</section>
<%- partial('_partial/base-paginator') %>