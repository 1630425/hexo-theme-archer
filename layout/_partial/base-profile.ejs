<div class="profile">
    <img class="profile-avatar" src= <%- theme.avatar %> >
    <div class="site-name"><%- theme.author || config.author %></div>
    <% if(theme.signature && theme.signature.length !== 0) {%>
    <div class="profile-signature">
        <%= theme.signature %>
    </div>
    <% } %>
    <% if(theme.social) { %>
    <div class="profile-social">
        <%- partial('base-social') %>
    </div>
    <% } %>
    <% console.log(theme.friends) %>
    <% if(theme.friends) { %>
        <div class="friends">
            <div>FRIENDS</div>
            <% for (name in theme.friends) { %>
                <span>
                    <a href= <%- theme.friends[name] %> target="_black"><%- name %></a>
                </span>
            <% } %>
        </div>
    <% } %>
</div>