
<footer>
    <% for (name in theme.social) { %>
        <% if (theme.social[name] != null) { %>
        <% console.log(theme.social[name]); %>
            <% if (name === "email") { %>
                <a href = "emailto:<%- theme.social[name] %>" class = "iconfont <%- name %>" title = <%- name %> ></a>
            <% } else { %>
                <a href= <%- theme.social[name] %> class = "iconfont <%- name %>" title = <%- name %> ></a>
            <% } %>
        <% } %>
    <% } %>
</footer>